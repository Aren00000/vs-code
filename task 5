using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSLight
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string name = "x";
            string surname = "y";
            string trueVariable;

            Console.WriteLine($"name ={name} surname ={surname} ");
            trueVariable = name;
            name = surname;
            surname = name;
            Console.WriteLine($"name ={name} surname ={surname} ");
        }
    }
}
