using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSLight
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int pictures = 52;
            int picturesInRow = 3;
            int filledRows;
            int extraPictures;
            
            filledRows = pictures / picturesInRow;
            extraPictures = pictures % picturesInRow;
            
            Console.WriteLine($"У вас есть полностью заполненных рядов:{filledRows}, лишних картинок:{extraPictures}.");
        }
    }
}
